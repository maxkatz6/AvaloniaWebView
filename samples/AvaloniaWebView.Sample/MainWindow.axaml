<Window x:Class="AvaloniaWebView.Sample.MainWindow"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:webview="using:AvaloniaWebView"
        Title="AvaloniaWebView.Sample"
        d:DesignHeight="450"
        d:DesignWidth="800"
        mc:Ignorable="d">
  <TabControl>
    <TabItem Header="Google">
      <webview:NativeWebView Source="https://google.com" />
    </TabItem>
    <TabItem Header="Reddit">
      <webview:NativeWebView Source="https://reddit.com" />
    </TabItem>
  </TabControl>
</Window>
